#include "STD_TYPES.h"
#include "Delay.h"
#include "DIO_interface.h"
#include "PORT_interface.h"
#include "CLCD_interface.h"

#define F_CPU 1000000UL
#include <util/delay.h>

void main (void)
{
	u8 i;
	PortInitialize();
	CLCD_voidInitialize();
	LCD_voidWriteString("MG2");

	while(1)
	{
		_delay_ms(500);
		CLCD_voidCursorPosition(1, 3);
		CLCD_voidInitialize();
		LCD_voidWriteString("MG2");
		_delay_ms(500);
	}
}
