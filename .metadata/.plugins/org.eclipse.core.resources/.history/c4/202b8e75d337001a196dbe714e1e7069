#include "STD_TYPES.h"
#include "Delay.h"
#include "DIO_interface.h"
#include "PORT_interface.h"
#include "CLCD_interface.h"

#define F_CPU 800000UL
#include <util/delay.h>

void main (void)
{
	u8 i;
	PortInitialize();


	while(1)
	{
		CLCD_voidInitialize();
		LCD_voidWriteString("MG2");
		_delay_ms(1000);
		CLCD_voidInitialize();
		CLCD_voidCursorPosition(1, 3);
		LCD_voidWriteString("MG2");
		_delay_ms(1000);
		CLCD_voidInitialize();
		CLCD_voidCursorPosition(0, 6);
		LCD_voidWriteString("MG2");
		_delay_ms(1000);
		CLCD_voidInitialize();
		CLCD_voidCursorPosition(1, 9);
		LCD_voidWriteString("MG2");
		_delay_ms(1000);
		CLCD_voidInitialize();
		CLCD_voidCursorPosition(0, 12);
		LCD_voidWriteString("MG2");
		_delay_ms(1000);
		CLCD_voidInitialize();
		CLCD_voidCursorPosition(1, 9);
		LCD_voidWriteString("MG2");
		_delay_ms(1000);
		CLCD_voidInitialize();
		CLCD_voidCursorPosition(0, 6);
		LCD_voidWriteString("MG2");
		_delay_ms(1000);
	}
}
