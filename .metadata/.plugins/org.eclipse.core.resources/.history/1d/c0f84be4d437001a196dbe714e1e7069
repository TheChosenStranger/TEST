#include "STD_TYPES.h"
#include "Delay.h"
#include "DIO_interface.h"
#include "PORT_interface.h"
#include "CLCD_interface.h"

#define F_CPU 800000UL
#include <util/delay.h>

void Task1(void)
{
	CLCD_voidInitialize();
	LCD_voidWriteString("Marwa");
	_delay_ms(1000);
	CLCD_voidInitialize();
	CLCD_voidCursorPosition(1, 3);
	LCD_voidWriteString("Marwa");
	_delay_ms(1000);
	CLCD_voidInitialize();
	CLCD_voidCursorPosition(0, 6);
	LCD_voidWriteString("Marwa");
	_delay_ms(1000);
	CLCD_voidInitialize();
	CLCD_voidCursorPosition(1, 9);
	LCD_voidWriteString("Marwa");
	_delay_ms(1000);
	CLCD_voidInitialize();
	CLCD_voidCursorPosition(0, 12);
	LCD_voidWriteString("Marwa");
	_delay_ms(1000);
	CLCD_voidInitialize();
	CLCD_voidCursorPosition(1, 9);
	LCD_voidWriteString("Marwa");
	_delay_ms(1000);
	CLCD_voidInitialize();
	CLCD_voidCursorPosition(0, 6);
	LCD_voidWriteString("Marwa");
	_delay_ms(1000);
	CLCD_voidInitialize();
	CLCD_voidCursorPosition(1, 3);
	LCD_voidWriteString("Marwa");
	_delay_ms(1000);
}

void Task2(void)
{

}
void main (void)
{
	PortInitialize();
	CLCD_voidInitialize();
	LCD_voidWriteString("Password:");

	while(1)
	{
		Task1();
	}
}
